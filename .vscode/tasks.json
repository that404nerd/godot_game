{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "build_module",
      "type": "shell",
      "command": "cmake --build ${workspaceFolder}/build/FPSProject-build --config Debug -j8",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "problemMatcher": [
        "$gcc"
      ]
    },
    {
      "label": "install_module",
      "type": "shell",
      "command": "cmake --install ${workspaceFolder}/build/FPSProject-build",
      "options": {
        "cwd": "${workspaceFolder}"
      },
      "group": {
        "kind": "build",
        "isDefault": false
      },
      "problemMatcher": []
    }
  ]
}
